{
  "expo": {
    "name": "Mario",
    "slug": "mario",
    "splash": {
      "image": "./assets/images/splashh.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Super-Mario.png",
    "scheme": "mario",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/Super-Mario.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.varun199.mario"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d2979051-824a-455a-9776-9262c11d27c3"
      }
    },
    "owner": "varun199"
  }
}
